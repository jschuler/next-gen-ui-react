{
  "name": "next-gen-ui-react-demo",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npm run build:standalone && npm run build:webcomponents && npm run copy-bundles && vite",
    "copy-bundles": "cp -r dist/standalone public/ && cp -r dist/webcomponents public/",
    "build": "vite build",
    "build:netlify": "VITE_BASE_PATH=/ npm run build:all",
    "build:standalone": "vite build --config configs/vite.config.standalone.ts && node scripts/update-cache-version.js",
    "build:webcomponents": "vite build --config configs/vite.config.webcomponents.ts && node scripts/update-cache-version.js",
    "build:all": "npm run build && npm run build:standalone && npm run build:webcomponents",
    "preview": "vite preview",
    "lint": "cd .. && npx eslint app/",
    "lint:fix": "cd .. && npx eslint app/ --fix",
    "format": "cd .. && npx prettier --write \"app/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "cd .. && npx prettier --check \"app/**/*.{ts,tsx,js,jsx,json,css,md}\""
  },
  "dependencies": {
    "@patternfly/react-core": "^6.4.0",
    "@patternfly/react-icons": "^6.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0"
  },
  "devDependencies": {
    "@types/node": "^24.9.2",
    "@vitejs/plugin-react": "^4.7.0",
    "typescript": "~5.8.3",
    "vite": "^7.1.0"
  }
}
